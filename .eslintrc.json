{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "extends": [
        "standard",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warn", {
            "singleQuote": true,
            "trailingComma": "never",
            "bracketSpacing": true,
            "semi": false
        }]
    }
}
